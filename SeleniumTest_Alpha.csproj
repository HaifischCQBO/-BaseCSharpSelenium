      <PackageReference Include="Selenium.DefaultWaitHelpers" Version="1.0.3" />
      <PackageReference Include="Selenium.Support" Version="4.2.0" />
      <PackageReference Include="Selenium.WebDriver" Version="4.2.0" />
      <PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="102.0.5005.6102" />
      <PackageReference Include="System.Configuration.ConfigurationManager" Version="6.0.0" />
      <PackageReference Include="System.Runtime" Version="4.3.1" />
      <PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="6.0.0" />
      <PackageReference Include="TestProject.SDK" Version="0.65.0" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="SeleniumTestLocal\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="SeleniumTestLocal\**" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="SeleniumTestLocal\**" />
